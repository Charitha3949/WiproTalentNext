package mycodes;
import java.util.*;
public class NUmberCheck {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("Enter a Number: ");
		int a = s.nextInt();
		if (a>0) {
			System.out.print("Positive");
		}
		else if(a<0) {
			System.out.print("Negative");
		}
		else {
			System.out.print("Zero");
		}
	}

}
